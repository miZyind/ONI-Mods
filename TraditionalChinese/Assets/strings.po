msgctxt "STRINGS.CODEX.PACU.SUBTITLE"
msgid "Aquatic Critter"
msgstr "水生動物"

#. STRINGS.CODEX.PACU.TITLE
msgctxt "STRINGS.CODEX.PACU.TITLE"
msgid "Pacu"
msgstr "帕庫魚"

